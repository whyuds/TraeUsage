{
  "name": "trae-usage-monitor",
  "displayName": "Trae Usage",
  "description": "Monitor Trae AI usage statistics in real-time",
  "version": "1.0.2",
  "publisher": "whyuds",
  "repository": {
    "type": "git",
    "url": "https://github.com/whyuds/TraeUsage.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "img/logo_256.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "traeUsage.refresh",
        "title": "Refresh Usage Data",
        "icon": "$(refresh)"
      },
      {
        "command": "traeUsage.updateToken",
        "title": "设置Token",
        "icon": "$(gear)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "traeUsageView",
          "name": "Trae Usage",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "traeUsage.refresh",
          "when": "view == traeUsageView",
          "group": "navigation"
        },
        {
          "command": "traeUsage.updateToken",
          "when": "view == traeUsageView",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Trae Usage",
      "properties": {
        "traeUsage.authToken": {
          "type": "string",
          "default": "",
          "description": "Trae AI authentication token"
        },
        "traeUsage.refreshInterval": {
          "type": "number",
          "default": 5,
          "description": "Refresh interval in minutes"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "18.x",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0"
  }
}